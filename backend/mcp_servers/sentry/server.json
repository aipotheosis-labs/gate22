{
  "name": "SENTRY",
  "url": "https://mcp.sentry.dev/mcp",
  "transport_type": "streamable_http",
  "description": "Sentry provides end-to-end distributed tracing, enabling developers to identify and debug performance issues and errors across their systems and services.",
  "logo": "https://raw.githubusercontent.com/aipotheosis-labs/aipolabs-icons/refs/heads/main/apps/sentry.svg",
  "categories": ["Developer Tools", "Monitoring", "Error Tracking"],
  "auth_configs": [
    {
      "type": "oauth2",
      "client_id": "{{ CLIENT_ID }}",
      "client_secret": "{{ CLIENT_SECRET }}",
      "scope": "",
      "authorize_url": "https://mcp.sentry.dev/oauth/authorize",
      "access_token_url": "https://mcp.sentry.dev/oauth/token",
      "refresh_token_url": "https://mcp.sentry.dev/oauth/token"
    }
  ],
  "server_metadata": {}
}
