                      },
                      "currency": {
                        "type": "string"
                      }
                    },
                    "additionalProperties": false
                  }
                },
                "additionalProperties": false
              }
            },
            "paymentAccount": {
              "type": "object",
              "required": ["accountNumber", "accountHolder", "paymentMethod"],
