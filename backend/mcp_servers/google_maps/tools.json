    "input_schema": {
      "type": "object",
      "required": ["query"],
      "properties": {
        "query": {
          "type": "object",
          "required": ["locations"],
          "properties": {
            "locations": {
              "type": "string",
              "example": "35.68,139.76|40.71,-74.01",
              "pattern": "^([-+]?([1-8]?\\d(\\.\\d+)?|90(\\.0+)?),[-+]?(180(\\.0+)?|((1[0-7]\\d)|([1-9]?\\d))(\\.\\d+)?)(\\|[-+]?([1-8]?\\d(\\.\\d+)?|90(\\.0+)?),[-+]?(180(\\.0+)?|((1[0-7]\\d)|([1-9]?\\d))(\\.\\d+)?))*)$",
              "description": "Pipe-separated latitude/longitude coordinate pairs"
            }
          },
          "additionalProperties": false
        }
      },
      "additionalProperties": false
    }
