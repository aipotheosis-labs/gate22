{
  "name": "GOOGLE_ANALYTICS_ADMIN",
  "url": "http://mcp.aci.dev/virtual/mcp?server_name=GOOGLE_ANALYTICS_ADMIN",
  "transport_type": "streamable_http",
  "description": "The Google Analytics Admin API allows for programmatic access to the Google Analytics configuration data and is only compatible with Google Analytics properties.",
  "logo": "https://raw.githubusercontent.com/aipotheosis-labs/aipolabs-icons/refs/heads/main/apps/google_analytics.svg",
  "categories": ["Data & Analytics", "Marketing"],
  "auth_configs": [
    {
      "type": "oauth2",
      "client_id": "{{ CLIENT_ID }}",
      "client_secret": "{{ CLIENT_SECRET }}",
      "scope": "https://www.googleapis.com/auth/analytics.readonly https://www.googleapis.com/auth/analytics.edit",
      "authorize_url": "https://accounts.google.com/o/oauth2/v2/auth",
      "access_token_url": "https://oauth2.googleapis.com/token",
      "refresh_token_url": "https://oauth2.googleapis.com/token"
    }
  ],
  "server_metadata": {
    "is_virtual_mcp_server": true
  }
}
