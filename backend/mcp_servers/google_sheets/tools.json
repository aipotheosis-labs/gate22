    },
    "input_schema": {
      "type": "object",
      "required": ["path"],
      "properties": {
        "path": {
          "type": "object",
          "required": ["spreadsheetId", "range"],
          "properties": {
            "range": {
              "type": "string",
              "description": "The A1 notation or R1C1 notation of the range to retrieve values from"
            },
            "spreadsheetId": {
              "type": "string",
              "description": "The ID of the spreadsheet to retrieve data from"
            }
          },
          "additionalProperties": false
        },
        "query": {
          "type": "object",
          "required": [],
