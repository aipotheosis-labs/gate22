            "to",
            "subject"
          ],
          "visible": [
            "from",
            "to",
            "subject",
            "bcc",
            "cc",
            "scheduled_at",
            "reply_to",
            "html",
            "text",
            "tags"
          ],
          "additionalProperties": false
        }
