  {
    "name": "CAL__CREATE_SCHEDULE",
    "description": "Create a new schedule in Cal.com",
    "tool_metadata": {
      "type": "rest",
      "method": "POST",
      "endpoint": "https://api.cal.com/v2/schedules"
    },
    "input_schema": {
      "type": "object",
      "properties": {
        "body": {
          "type": "object",
          "properties": {
            "name": {
              "type": "string",
              "description": "Name of the schedule"
            },
            "timeZone": {
              "type": "string",
              "description": "Timezone for the schedule in IANA format",
              "example": "Asia/Shanghai"
            },
            "availability": {
              "type": "array",
              "description": "Availability time slots for the schedule",
              "items": {
                "type": "object",
                "properties": {
                  "days": {
                    "type": "array",
                    "description": "Days of the week for this availability",
                    "items": {
                      "type": "string",
                      "enum": [
                        "Monday",
                        "Tuesday",
                        "Wednesday",
                        "Thursday",
                        "Friday",
                        "Saturday",
                        "Sunday"
                      ]
                    }
                  },
                  "startTime": {
                    "type": "string",
                    "description": "Start time in HH:MM format",
                    "example": "09:00"
                  },
                  "endTime": {
                    "type": "string",
                    "description": "End time in HH:MM format",
                    "example": "17:00"
                  }
                }
              }
            },
            "isDefault": {
              "type": "boolean",
              "description": "Whether this is the default schedule",
              "default": false
            }
          },
          "required": [
            "name",
            "timeZone"
          ],
          "visible": [
            "name",
            "timeZone",
            "availability",
            "isDefault"
          ],
          "additionalProperties": false
        }
      },
      "required": [
        "body"
      ],
      "visible": [
        "body"
      ],
      "additionalProperties": false
    }
  },
